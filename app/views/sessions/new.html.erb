<div class="container">
  	<div class="row">
	    <% content_for :title do %>Access Project<% end %>
	    <h3>Access Project</h3>
	    <div class="form">
			<%= form_for(:session, url: login_path) do |f| %>
				<%= f.text_field :name, :placeholder => "Name" %>
				<%= f.text_field :password, :placeholder => "Password" %>
				<%= f.submit "Access Project" %>
			<% end %>
		</div>
	</div>
</div>